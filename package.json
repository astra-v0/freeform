{
  "name": "@astra-v0/survey-builder",
  "version": "1.0.0",
  "description": "Programmatic survey builder inspired by Typeform style",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "type-check": "tsc --noEmit",
    "lint": "eslint . --ext .ts,.tsx",
    "lint:fix": "eslint . --ext .ts,.tsx --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "ci": "npm run type-check && npm run lint && npm run format:check && npm run build && npm test && npm run demo:build",
    "prepublishOnly": "npm run build",
    "test": "jest --passWithNoTests",
    "demo": "cd demo-react && npm run dev",
    "demo:build": "cd demo-react && npm run build",
    "demo:preview": "cd demo-react && npm run preview"
  },
  "keywords": [
    "survey",
    "form",
    "typeform",
    "questionnaire",
    "typescript"
  ],
  "author": "Your Name",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^29.0.0",
    "@types/node": "^20.0.0",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "@vitejs/plugin-react": "^4.0.0",
    "eslint": "^8.57.0",
    "eslint-plugin-react": "^7.34.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "jest": "^29.0.0",
    "prettier": "^3.2.0",
    "ts-jest": "^29.0.0",
    "typescript": "^5.0.0",
    "vite": "^5.0.0"
  },
  "dependencies": {
    "lucide-react": "^0.545.0",
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0"
  },
  "peerDependencies": {
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0"
  },
  "files": [
    "dist/**/*",
    "README.md"
  ]
}
